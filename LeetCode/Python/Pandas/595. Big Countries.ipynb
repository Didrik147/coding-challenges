   "source": [
    "import pandas as pd\n",
    "\n",
    "def big_countries(world: pd.DataFrame) -> pd.DataFrame:\n",
    "  big = world[ (world['population'] >= 25E6) | (world['area'] >= 3E6) ]\n",
    "  return big[['name', 'population', 'area']]"
   ]
  },
